<Window x:Class="Yarsey.Desktop.WPF.WindowSetupCompany"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Yarsey.Desktop.WPF"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
        xmlns:syncfusionskin ="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
        syncfusionskin:SfSkinManager.Theme="{syncfusionskin:SkinManagerExtension ThemeName=FluentDarkTurq}"
        mc:Ignorable="d"
        Title="Yarsey Desktop Akaun" Height="800" Width="1000" WindowStartupLocation="CenterScreen">
    <Grid>
        <syncfusion:WizardControl Name="wizardControl" NextVisible="False" >
            <syncfusion:WizardPage Name="Welcome" >
                <Label Content="Page 1"/>
            </syncfusion:WizardPage>
            <syncfusion:WizardPage Name="ConfigureCompany">
                <Label Content="Page 2"/>
            </syncfusion:WizardPage>

        </syncfusion:WizardControl>
    </Grid>
</Window>
