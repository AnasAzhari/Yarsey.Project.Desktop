<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Yarsey.WPF">

    <Color x:Key="Color1">#FFCBF2</Color>
    <Color x:Key="Color2">#F3C4FB</Color>
    <Color x:Key="Color3">#ECBCFD</Color>
    <Color x:Key="Color4">#E5B3FE</Color>
    <Color x:Key="Color5">#E2AFFF</Color>
    <Color x:Key="Color6">#DEAAFF</Color>
    <Color x:Key="Color7">#D8BBFF</Color>
    <Color x:Key="Color8">#D0D1FF</Color>
    <Color x:Key="Color9">#C8E7FF</Color>
    <Color x:Key="Color10">#C0FDFF</Color>
    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource Color1}"/>
    <SolidColorBrush x:Key="BrushPrimary2" Color="{StaticResource Color2}"/>
    <SolidColorBrush x:Key="BrushPrimary3" Color="{StaticResource Color3}"/>
    <SolidColorBrush x:Key="BrushPrimary4" Color="{StaticResource Color4}"/>
    <SolidColorBrush x:Key="BrushPrimary5" Color="{StaticResource Color5}"/>
    <SolidColorBrush x:Key="BrushPrimary6" Color="{StaticResource Color6}"/>
    <SolidColorBrush x:Key="BrushPrimary7" Color="{StaticResource Color7}"/>
    <SolidColorBrush x:Key="BrushPrimary8" Color="{StaticResource Color8}"/>
    <SolidColorBrush x:Key="BrushPrimary9" Color="{StaticResource Color9}"/>
    <SolidColorBrush x:Key="BrushPrimary10" Color="{StaticResource Color10}"/>

</ResourceDictionary>